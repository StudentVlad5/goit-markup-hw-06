{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "bracketSameLine": false,
  "plugins": [],
  "jsxSingleQuote": false,
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "css",
  "trailingSlash": "never",
  "parser": "html"
}
